<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!-- 错误提示窗体 start -->
<div id="errorTipModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="errorTipModalLabel" aria-hidden="true">
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
		<h3 id="errorTipModalLabel">错误提示:</h3>
	</div>
	<div class="modal-body">
		<div id="errorTipModalBody" style="color:red;"></div>
	</div>
	<div class="modal-footer">
		<button id="closeErrorTipModal" class="btn" data-dismiss="modal" aria-hidden="true">关闭</button>
	</div>
</div>   
<!-- 错误提示窗体 end -->   
